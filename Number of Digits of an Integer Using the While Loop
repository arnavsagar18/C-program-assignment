#include <stdio.h>
int main() {
    int no, count = 0;
    printf("Enter an integer: ");
    scanf("%d", &no);
    if (no == 0) {
        count = 1;
    } else {
        while (no != 0) {
            no = no / 10; 
            count++;
        }
    }
    printf("Number of digits: %d\n", count);
    return 0;
}
