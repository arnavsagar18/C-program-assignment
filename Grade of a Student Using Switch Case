#include <stdio.h>
int main() {
    int mrks;
    printf("Enter the marks obtained: ");
    scanf("%d", &mrks);
    char grade;
    switch (mrks / 10) {
        case 10:
        case 9:
            grade = 'A';
            break;
        case 8:
            grade = 'B';
            break;
        case 7:
            grade = 'C';
            break;
        case 6:
            grade = 'D';
            break;
        default:
            grade = 'F';
    }
    printf("Grade: %c\n", grade);
    return 0;
}
